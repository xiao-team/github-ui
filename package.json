    "url-loader": "^1.1.2",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.3.0",
    "webpack-merge": "^4.1.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,json,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "es6-promise": "^4.2.5",
